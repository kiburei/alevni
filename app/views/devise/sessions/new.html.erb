<h2><%= link_to image_tag("login.png", height: 70, alt: "Home"),'/' %></h2>



<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field login">
    <%= f.label :member_code %><br />
    <%= f.text_field :member_code, autofocus: true  %>
  </div>

  <div class="field login">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>
<button class="btn btn-success" name="button"><a href="http://invela.co.ke/10k-club-properties.html" target="_blank">Sign Up</a></button></br>


<%= render "devise/shared/links" %>
